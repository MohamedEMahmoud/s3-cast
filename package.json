{
    "name": "s3",
    "version": "0.1.0",
    "description": "s3-cast",
    "main": "index.ts",
    "scripts": {
        "start": "nodemon src/index.ts",
        "build": "npx tsc",
        "tsc": "tsc",
        "create:dev:db": "db-migrate db:create s3 --config database.json -e conn && npm run db:up",
        "db:up": "db-migrate up",
        "db:down": "db-migrate down",
        "format": "prettier --write src/*.ts",
        "lint": "eslint src/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "author": "MohamedEMahmoud",
    "license": "ISC",
    "dependencies": {
        "@types/multer": "^1.4.7",
        "aws-sdk": "^2.1122.0",
        "axios": "^0.27.2",
        "cookie-session": "^2.0.0",
        "cross-env": "^7.0.3",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.4",
        "npm-check-updates": "^12.5.9",
        "pg": "^8.7.1"
    },
    "devDependencies": {
        "@types/cookie-session": "^2.0.44",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.8",
        "@types/pg": "^8.6.3",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.15",
        "ts-node": "^10.4.0",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.5.4"
    }
}
